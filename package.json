{
  "name": "@agentofuser/ipfs-deploy",
  "version": "3.1.0",
  "description": "Pin directory locally, send hash to pinning service, and update DNS",
  "author": {
    "name": "Agent of User",
    "email": "git@agentofuser.com",
    "url": "https://agentofuser.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/agentofuser/ipfs-deploy/issues"
  },
  "homepage": "https://github.com/agentofuser/ipfs-deploy#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/agentofuser/ipfs-deploy.git"
  },
  "keywords": [
    "ipfs",
    "gatsby",
    "deploy"
  ],
  "publishConfig": {
    "access": "public"
  },
  "main": "index.js",
  "bin": {
    "ipfs-deploy": "bin/ipfs-deploy.js",
    "ipd": "bin/ipfs-deploy.js"
  },
  "dependencies": {
    "@pinata/sdk": "^1.0.16",
    "chalk": "^2.4.2",
    "clipboardy": "^1.2.3",
    "dnslink-cloudflare": "^2.0.1",
    "dotenv": "^7.0.0",
    "got": "^9.6.0",
    "ip": "^1.1.5",
    "ipfs": "^0.35.0-rc.4",
    "ipfsd-ctl": "^0.42.1",
    "lodash": "^4.17.11",
    "multiaddr": "^6.0.6",
    "open": "^6.0.0",
    "ora": "^3.4.0",
    "which": "^1.3.1",
    "yargs": "^13.2.2"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^3.0.2",
    "@semantic-release/commit-analyzer": "^7.0.0-beta.2",
    "@semantic-release/git": "^7.0.9",
    "@semantic-release/github": "^5.3.1",
    "@semantic-release/npm": "^5.1.6",
    "@semantic-release/release-notes-generator": "^7.1.6",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-import": "^2.16.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "ndb": "^1.0.44",
    "prettier": "^1.16.4",
    "semantic-git-commit-cli": "^3.2.2",
    "semantic-release": "^15.13.8",
    "semantic-release-cli": "^4.1.1"
  },
  "scripts": {
    "format": "npx prettier --write ./**/*.{js,json,md,mdx,html,css}",
    "test": "echo \"Error: no test specified\" && exit 1",
    "commit": "npx sgc",
    "semantic-release": "./semantic-release-dry-run.bash",
    "deploy": "./release.bash"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,md,mdx,html,css}": [
      "prettier --write",
      "git add"
    ],
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "sgc": {
    "lowercaseTypes": true
  }
}
